
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '7.0'

inhibit_all_warnings!

# I think a version number for each 3rd party library is required
# because a version update can cause a major changes in interface.

# Sorted the libraries alphabetically so that we can check easily.


def pods
    pod 'SDWebImage',                           '~> 4.2.2'
end

target 'QMSegmentContainerDemo' do
    pods
    
end

